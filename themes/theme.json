			}
		},
		{
			"scope": "markup.fenced_code.block",
			"settings": {
				"foreground": "#9b9b9b"
			}
		},
		{
			"scope": [
				"markup.inserted",
				"meta.diff.header.to-file"
			],
			"settings": {
				"foreground": "#85e89d"
			}
		},
		{
			"scope": [
				"markup.deleted",
				"meta.diff.header.from-file"
			],
			"settings": {
				"foreground": "#ff6b88"
			}
		},
		{
			"scope": "markup.changed",
			"settings": {
				"foreground": "#ffab70"
			}
		},
		{
			"scope": "meta.diff.range",
			"settings": {
				"foreground": "#b392f0",
				"fontStyle": "bold"
			}
		},
		{
			"scope": "meta.separator",
			"settings": {
				"foreground": "#79b8ff",
				"fontStyle": "bold"
			}
		},
		{
			"scope": "punctuation.definition.list.begin.markdown",
			"settings": {
				"foreground": "#ffab70"
			}
		},
		{
			"scope": [
				"constant.other.reference.link",
				"string.other.link"
			],
			"settings": {
				"foreground": "#5fcfef",
				"fontStyle": "underline"
			}
		},
		{
			"scope": "punctuation.accessor",
			"settings": {
				"foreground": "#e4e4e4"
			}
		},
		{
			"scope": "punctuation.separator",
			"settings": {
				"foreground": "#9b9b9b"
			}
