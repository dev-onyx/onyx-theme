	"tokenColors": [
		{
			"scope": [
				"comment",
				"punctuation.definition.comment",
				"string.comment"
			],
			"settings": {
				"foreground": "#6b6b6b",
				"fontStyle": "italic"
			}
		},
		{
			"scope": [
				"constant",
				"entity.name.constant",
				"variable.other.constant",
				"variable.language.this",
				"variable.language.super",
				"variable.language.self"
			],
			"settings": {
				"foreground": "#79b8ff"
			}
		},
		{
			"scope": [
				"entity",
				"entity.name"
			],
			"settings": {
				"foreground": "#b392f0"
			}
		},
		{
			"scope": "entity.name.function",
			"settings": {
				"foreground": "#b392f0"
			}
		},
		{
			"scope": "entity.name.type",
			"settings": {
				"foreground": "#ff9966"
			}
		},
		{
			"scope": "entity.name.tag",
			"settings": {
				"foreground": "#85e89d"
			}
		},
		{
			"scope": "entity.other.attribute-name",
			"settings": {
				"foreground": "#79b8ff"
			}
		},
		{
			"scope": [
				"keyword",
				"storage.modifier",
				"storage.type.class",
				"storage.type.function"
			],
			"settings": {
				"foreground": "#f97583"
			}
		},
		{
			"scope": [
				"storage",
				"storage.type"
			],
			"settings": {
				"foreground": "#f97583"
			}
		},
		{
			"scope": [
				"string",
				"punctuation.definition.string",
				"string punctuation.section.embedded source"
			],
			"settings": {
				"foreground": "#9ecbff"
			}
		},
		{
			"scope": "string.regexp",
			"settings": {
