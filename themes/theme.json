			}
		},
		{
			"scope": [
				"support.type",
				"support.class"
			],
			"settings": {
				"foreground": "#ff9966"
			}
		},
		{
			"scope": [
				"support.function",
				"support.constant"
			],
			"settings": {
				"foreground": "#79b8ff"
			}
		},
		{
			"scope": "support.variable",
			"settings": {
				"foreground": "#79b8ff"
			}
		},
		{
			"scope": "meta.property-name",
			"settings": {
				"foreground": "#79b8ff"
			}
		},
		{
			"scope": [
				"variable",
				"variable.parameter"
			],
			"settings": {
				"foreground": "#e4e4e4"
			}
		},
		{
			"scope": "variable.other.property",
			"settings": {
				"foreground": "#ffab70"
			}
		},
		{
			"scope": "variable.function",
			"settings": {
				"foreground": "#b392f0"
			}
		},
		{
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#ff6b88",
				"fontStyle": "italic"
			}
		},
		{
			"scope": "invalid.deprecated",
			"settings": {
				"foreground": "#ff6b88",
				"fontStyle": "strikethrough italic"
			}
		},
		{
			"scope": "markup.heading",
			"settings": {
				"foreground": "#79b8ff",
				"fontStyle": "bold"
			}
		},
		{
			"scope": "markup.bold",
			"settings": {
				"foreground": "#e4e4e4",
				"fontStyle": "bold"
			}
		},
		{
			"scope": "markup.italic",
			"settings": {
				"foreground": "#e4e4e4",
				"fontStyle": "italic"
			}
		},
		{
			"scope": "markup.underline",
			"settings": {
				"fontStyle": "underline"
			}
		},
		{
			"scope": "markup.strikethrough",
			"settings": {
				"fontStyle": "strikethrough"
			}
		},
		{
